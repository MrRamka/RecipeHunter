<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<div class="fruits-container">
    <div class="custom-flex">
        <div class="">
            <img src="/resourses/img/cherries.png" alt="">
        </div>
        <div class="">
            <h2 class="container-title">Fruits</h2>
        </div>
    </div>
    <div class="fruits-items">
        <div class="custom-control form-control-lg custom-checkbox">
            <input type="checkbox" class="custom-control-input" id="orange-check" name="orange-check">
            <label class="custom-control-label" for="orange-check">Orange</label>
        </div>
        <div class="custom-control form-control-lg custom-checkbox">
            <input type="checkbox" class="custom-control-input" id="lemon-check" name="lemon-check">
            <label class="custom-control-label" for="lemon-check">Lemon</label>
        </div>
        <div class="custom-control form-control-lg custom-checkbox">
            <input type="checkbox" class="custom-control-input" id="lime-check" name="lime-check">
            <label class="custom-control-label" for="lime-check">Lime</label>
        </div>
        <div class="custom-control form-control-lg custom-checkbox">
            <input type="checkbox" class="custom-control-input" id="banana-check" name="banana-check">
            <label class="custom-control-label" for="banana-check">Banana</label>
        </div>
        <div class="custom-control form-control-lg custom-checkbox">
            <input type="checkbox" class="custom-control-input" id="apple-check" name="apple-check">
            <label class="custom-control-label" for="apple-check">Apple</label>
        </div>
        <div class="custom-control form-control-lg custom-checkbox">
            <input type="checkbox" class="custom-control-input" id="peach-check" name="peach-check">
            <label class="custom-control-label" for="peach-check">Peach</label>
        </div>
    </div>
</div>