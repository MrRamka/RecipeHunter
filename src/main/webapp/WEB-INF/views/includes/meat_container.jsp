<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<div class="meat-container">
    <div class="custom-flex">
        <div class="">
            <img src="/resourses/img/steak.png" alt="">
        </div>
        <div class="">
            <h2 class="container-title">Meat</h2>
        </div>
    </div>
    <div class="meat-items">
        <div class="custom-control form-control-lg custom-checkbox">
            <input type="checkbox" class="custom-control-input" id="beef-check" name="beef-check">
            <label class="custom-control-label" for="beef-check">Beef</label>
        </div>
        <div class="custom-control form-control-lg custom-checkbox">
            <input type="checkbox" class="custom-control-input" id="chicken-check" name="chicken-check">
            <label class="custom-control-label" for="chicken-check">Chicken</label>
        </div>
        <div class="custom-control form-control-lg custom-checkbox">
            <input type="checkbox" class="custom-control-input" id="fish-check" name="fish-check">
            <label class="custom-control-label" for="fish-check">Fish</label>
        </div>
        <div class="custom-control form-control-lg custom-checkbox">
            <input type="checkbox" class="custom-control-input" id="pork-check" name="pork-check">
            <label class="custom-control-label" for="pork-check">Pork</label>
        </div>
        <div class="custom-control form-control-lg custom-checkbox">
            <input type="checkbox" class="custom-control-input" id="turkey-check" name="turkey-check">
            <label class="custom-control-label" for="turkey-check">Turkey</label>
        </div>
        <div class="custom-control form-control-lg custom-checkbox">
            <input type="checkbox" class="custom-control-input" id="lamb-check" name="lamb-check">
            <label class="custom-control-label" for="lamb-check">Lamb</label>
        </div>
    </div>
</div>