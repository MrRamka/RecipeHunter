<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<div class="vegetables-container">
    <div class="custom-flex">
        <div class="">
            <img src="/resourses/img/tomato.png" alt="">
        </div>
        <div class="">
            <h2 class="container-title">Vegetables</h2>
        </div>
    </div>
    <div class="vegetables-items">
        <div class="custom-control form-control-lg custom-checkbox">
            <input type="checkbox" class="custom-control-input" id="potato-check" name="potato-check">
            <label class="custom-control-label" for="potato-check">Potato</label>
        </div>
        <div class="custom-control form-control-lg custom-checkbox">
            <input type="checkbox" class="custom-control-input" id="onion-check" name="onion-check">
            <label class="custom-control-label" for="onion-check">Onion</label>
        </div>
        <div class="custom-control form-control-lg custom-checkbox">
            <input type="checkbox" class="custom-control-input" id="garlic-check" name="garlic-check">
            <label class="custom-control-label" for="garlic-check">Garlic</label>
        </div>
        <div class="custom-control form-control-lg custom-checkbox">
            <input type="checkbox" class="custom-control-input" id="cabbage-check" name="cabbage-check">
            <label class="custom-control-label" for="cabbage-check">Cabbage</label>
        </div>
        <div class="custom-control form-control-lg custom-checkbox">
            <input type="checkbox" class="custom-control-input" id="mushrooms-check" name="mushrooms-check">
            <label class="custom-control-label" for="mushrooms-check">Mushrooms</label>
        </div>
        <div class="custom-control form-control-lg custom-checkbox">
            <input type="checkbox" class="custom-control-input" id="carrot-check" name="carrot-check">
            <label class="custom-control-label" for="carrot-check">Carrot</label>
        </div>
        <div class="custom-control form-control-lg custom-checkbox">
            <input type="checkbox" class="custom-control-input" id="tomato-check" name="tomato-check">
            <label class="custom-control-label" for="tomato-check">Tomato</label>
        </div>
    </div>
</div>