<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<div class="daily-container">
    <div class="custom-flex">
        <div class="">
            <img src="/resourses/img/groceries.png" alt="">
        </div>
        <div class="">
            <h2 class="container-title">Daily</h2>
        </div>
    </div>
    <div class="daily-items">
        <div class="custom-control form-control-lg custom-checkbox">
            <input type="checkbox" class="custom-control-input" id="cheese-check" name="cheese-check">
            <label class="custom-control-label" for="cheese-check">Cheese</label>
        </div>
        <div class="custom-control form-control-lg custom-checkbox">
            <input type="checkbox" class="custom-control-input" id="egg-check" name="egg-check">
            <label class="custom-control-label" for="egg-check">Egg</label>
        </div>
        <div class="custom-control form-control-lg custom-checkbox">
            <input type="checkbox" class="custom-control-input" id="pasta-check" name="pasta-check">
            <label class="custom-control-label" for="pasta-check">Pasta</label>
        </div>
        <div class="custom-control form-control-lg custom-checkbox">
            <input type="checkbox" class="custom-control-input" id="rice-check" name="rice-check">
            <label class="custom-control-label" for="rice-check">Rice</label>
        </div>
        <div class="custom-control form-control-lg custom-checkbox">
            <input type="checkbox" class="custom-control-input" id="butter-check" name="butter-check">
            <label class="custom-control-label" for="butter-check">Butter</label>
        </div>
        <div class="custom-control form-control-lg custom-checkbox">
            <input type="checkbox" class="custom-control-input" id="milk-check" name="milk-check">
            <label class="custom-control-label" for="milk-check">Milk</label>
        </div>
        <div class="custom-control form-control-lg custom-checkbox">
            <input type="checkbox" class="custom-control-input" id="flour-check" name="flour-check">
            <label class="custom-control-label" for="flour-check">Flour</label>
        </div>
        <div class="custom-control form-control-lg custom-checkbox">
            <input type="checkbox" class="custom-control-input" id="salt-check" name="salt-check">
            <label class="custom-control-label" for="salt-check">Salt</label>
        </div>
        <div class="custom-control form-control-lg custom-checkbox">
            <input type="checkbox" class="custom-control-input" id="pepper-check" name="pepper-check">
            <label class="custom-control-label" for="pepper-check">Pepper</label>
        </div>
    </div>
</div>